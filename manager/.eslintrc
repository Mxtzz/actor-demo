{
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "@typescript-eslint",
    "react",
    "react-hooks"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "no-else-return": "error",
    "no-self-compare": "error",
    "no-void": "off",
    "no-var": "off",
    "no-lonely-if": "error",
    "prefer-const": "error",
    "react/prop-types": "off",
    "react/no-unknown-property": [
      "error",
      {
        "ignore": [
          "class"
        ]
      }
    ],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "off",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  },
  "ignorePatterns": [
    "./node_modules /*",
    "**/build /**/*",
    "**/public /**/*",
    "**/dist /**/*"
  ]
}